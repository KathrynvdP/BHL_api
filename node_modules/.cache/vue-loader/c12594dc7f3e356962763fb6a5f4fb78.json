   IonItem,\n    IonLabel,\n    IonSelect,\n    IonSelectOption,\n    IonInput,\n    BookList,\n  },\n  data() {\n    return {\n      category: \"\",\n      term: \"\",\n      books: [],\n    };\n  },\n  mounted() {\n    const route = useRoute();\n    const { id } = route.params;\n    console.log(id);\n    this.category = \"collection\";\n    this.term = id;\n    this.getAll(\"collection\", id);\n  },\n  methods: {\n    async getAll(category, term) {\n      this.loading = true;\n      let resp = await getAllBooks(category, term);\n      console.log(resp);\n      this.books = resp;\n      this.loading = false;\n    }\n  },\n};\n</script>\n"]}]}