ection\";\n    this.term = id;\n    this.getAll(\"collection\", id);\n  },\n  methods: {\n    async getAll(category, term) {\n      this.loading = true;\n      let resp = await getAllBooks(category, term);\n      console.log(resp);\n      this.books = resp;\n      this.loading = false;\n    }\n  },\n};\n</script>\n"]}]}