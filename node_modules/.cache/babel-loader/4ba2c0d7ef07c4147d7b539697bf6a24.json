ourites: [],\n    };\n  },\n  mutations: {\n    addFavourite(state, book) {\n      const newFave = {\n        id: book.Id,\n        title: book.Title,\n      };\n      state.favourites.unshift(newFave);\n    },\n  },\n  actions: {\n    addMemory(context, book) {\n      context.commit(\"addFavourite\", book);\n    },\n  },\n  getters: {\n    favourites(state) {\n      return state.favourites;\n    },\n    memory(state) {\n      return (memoryId) => {\n        return state.memories.find((memory) => memory.id === memoryId);\n      };\n    },\n  },\n});\n\nexport default store;\n"]}]}